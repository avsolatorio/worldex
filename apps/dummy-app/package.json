  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "eslint './**/*.{js,jsx}' --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "hygen": "hygen",
    "updateSupportedBrowsers": "echo \"var browserRegex = $(npx browserslist-useragent-regexp --allowHigherVersions);\" > public/supportedBrowsers.js"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "warn",
      "no-console": "warn",
      "no-debugger": "warn"
    },
    "extends": [
      "react-app",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "@deck.gl/core": "^8.9.19",
    "@deck.gl/layers": "^8.9.19",
    "@deck.gl/extensions": "^8.9.19",
    "@deck.gl/geo-layers": "^8.9.19",
    "@luma.gl/core": "^8.5.20"
  },
  "resolutions": {
    "@deck.gl/core": "^8.9.19",
    "@deck.gl/layers": "^8.9.19",
    "@deck.gl/extensions": "^8.9.19",
    "@deck.gl/geo-layers": "^8.9.19",
    "@luma.gl/core": "^8.5.20"
  },
  "prettier": {
    "printWidth": 90,
    "arrowParens": "always",
    "semi": true,
    "tabWidth": 2,
    "singleQuote": true,
    "jsxSingleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "yarn lint --max-warnings 0",
      "git add"
    ],
    "*.+(js|jsx|json|css|md)": [
      "yarn format",
      "git add"
    ]
  }
}
